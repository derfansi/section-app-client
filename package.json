{
  "name": "@tumi/legacy-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently --kill-others-on-fail \"yarn start\" \"yarn codegen --watch\"",
    "dev:light": "concurrently --kill-others-on-fail \"yarn start --configuration=light\" \"yarn codegen --watch\"",
    "start": "ng serve",
    "build": "ng build && yarn sentry:sourcemaps",
    "build:no-maps": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "prettier --write .",
    "codegen": "yarn graphql-codegen",
    "codegen:watch": "yarn graphql-codegen --watch",
    "lint": "ng lint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org esn-tumi-ev --project legacy-app ./dist/legacy-app && sentry-cli sourcemaps upload --org esn-tumi-ev --project legacy-app ./dist/legacy-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.7",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.7",
    "@angular/compiler": "^17.0.7",
    "@angular/core": "^17.0.7",
    "@angular/forms": "^17.0.7",
    "@angular/google-maps": "^17.0.4",
    "@angular/material": "^17.0.4",
    "@angular/material-luxon-adapter": "^17.0.4",
    "@angular/platform-browser": "^17.0.7",
    "@angular/platform-browser-dynamic": "^17.0.7",
    "@angular/router": "^17.0.7",
    "@angular/service-worker": "^17.0.7",
    "@apollo/client": "3.8.8",
    "@auth0/auth0-angular": "^2.2.2",
    "@azure/storage-blob": "^12.17.0",
    "@ng-web-apis/common": "^3.0.6",
    "@sentry/angular-ivy": "^7.88.0",
    "apollo-angular": "^6.0.0",
    "backoff-rxjs": "^7.0.0",
    "graphql": "^16.8.1",
    "highcharts": "^11.2.0",
    "highcharts-angular": "^4.0.0",
    "luxon": "^3.4.4",
    "marked": "^11.1.0",
    "ngx-markdown": "^17.1.1",
    "qr-scanner": "^1.4.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.7",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@parcel/watcher": "^2.3.0",
    "@sentry/cli": "^2.23.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/luxon": "^3.3.7",
    "@types/node": "^20.10.4",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jasmine-core": "~5.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.2.2"
  },
  "volta": {
    "node": "20.9.0"
  },
  "packageManager": "yarn@3.6.1"
}
