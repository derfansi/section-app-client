{
  "name": "@tumi/legacy-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "concurrently --kill-others-on-fail \"yarn start\" \"yarn codegen --watch\"",
    "dev:light": "concurrently --kill-others-on-fail \"yarn start --configuration=light\" \"yarn codegen --watch\"",
    "start": "ng serve",
    "build": "ng build && yarn sentry:sourcemaps",
    "build:no-maps": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "codegen": "yarn graphql-codegen",
    "codegen:watch": "yarn graphql-codegen --watch",
    "lint": "ng lint",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org esn-tumi-ev --project legacy-app ./dist/legacy-app && sentry-cli sourcemaps upload --org esn-tumi-ev --project legacy-app ./dist/legacy-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.7",
    "@angular/cdk": "^16.2.6",
    "@angular/common": "^16.2.7",
    "@angular/compiler": "^16.2.7",
    "@angular/core": "^16.2.7",
    "@angular/forms": "^16.2.7",
    "@angular/google-maps": "^16.2.6",
    "@angular/material": "^16.2.6",
    "@angular/material-luxon-adapter": "^16.2.6",
    "@angular/platform-browser": "^16.2.7",
    "@angular/platform-browser-dynamic": "^16.2.7",
    "@angular/router": "^16.2.7",
    "@angular/service-worker": "^16.2.7",
    "@apollo/client": "3.8.4",
    "@auth0/auth0-angular": "^2.2.1",
    "@azure/storage-blob": "^12.16.0",
    "@ng-web-apis/common": "^3.0.3",
    "@sentry/angular-ivy": "^7.72.0",
    "apollo-angular": "^5.0.2",
    "backoff-rxjs": "^7.0.0",
    "graphql": "^16.8.1",
    "highcharts": "^11.1.0",
    "highcharts-angular": "^3.1.2",
    "luxon": "^3.4.3",
    "marked": "^9.0.3",
    "ngx-markdown": "^16.0.0",
    "qr-scanner": "^1.4.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.4",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.4",
    "@angular/compiler-cli": "^16.2.7",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@sentry/cli": "^2.21.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.10",
    "@types/d3-scale": "^4.0.5",
    "@types/d3-selection": "^3.0.7",
    "@types/jasmine": "~4.3.6",
    "@types/luxon": "^3.3.2",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.7.1",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "jasmine-core": "~5.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6"
  },
  "volta": {
    "node": "20.5.0"
  },
  "packageManager": "yarn@3.6.1"
}
